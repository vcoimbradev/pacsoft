create table clientes(
    id int primary key auto_increment,
    razao_social varchar(255),
    cnpj int (14), 
    endereco varchar(255),
    bairro varchar(30),
    cep int(7),
    cidade varchar(100),
    estado varchar (30),
    telefone INT(11),
    inscricao_estadual int(12)
