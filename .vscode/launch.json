{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "vscode-edge-devtools.debug",
            "request": "launch",
            "name": "Launch Microsoft Edge and open the Edge DevTools",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Flutter Web with Edge DevTools",
            "type": "dart",
            "request": "launch",
            "program": "REPRESENTANTE/pacsoft_representante/lib/main.dart",
            "deviceId": "edge",
            "args": [
                "--web-browser-flag=--remote-debugging-port=9222"
            ]
        },
        {
            "type": "pwa-msedge",
            "name": "Launch Microsoft Edge",
            "request": "launch",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ],
            "url": "http://localhost:8000",
            "presentation": {
                "hidden": true
            }
        },
        {
            "type": "vscode-edge-devtools.debug",
            "name": "Open Edge DevTools",
            "request": "attach",
            "url": "http://localhost:8000",
            "presentation": {
                "hidden": true
            }
        }
    ],
    "compounds": [
        {
            "name": "Launch Flutter Web and Attach Edge DevTools",
            "configurations": [
                "Flutter Web with Edge DevTools",
                "Launch Microsoft Edge",
                "Open Edge DevTools"
            ]
        }
    ]
}
